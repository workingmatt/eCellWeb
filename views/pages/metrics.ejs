<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>
<% include ../partials/nav.ejs %>

<div class="container page" id="metrics">
	<h2>Metrics Page</h2>
	<table>
		<tr>
			<th>id</th>
			<th>name</th>
		</tr>
	    <% results.forEach(function(r) { %>
	    	<tr>
	    		<td><%= r.id %></td>
	    		<td><%= r.name %></td>
	    		<td><button>Show Record </button></td>
	    		<td><button>Delete Record </button></td>
	    	</tr>
	    <% }); %>
	</table>

</div>
</body>

<script type="text/javascript" src="javascript/metrics.js"></script>
</html>
