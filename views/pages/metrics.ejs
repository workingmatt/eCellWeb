<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>
<% include ../partials/nav.ejs %>

<div class="container page" id="metrics">
	<h2>Metrics Page</h2>
	<table>
		<tr>
			<th>id</th>
			<th>Page Opened</th>
			<th>1</th>
			<th>1</th>
			<th>1</th>
			<th>1</th>
			<th>1</th>
			<th>1</th>
			<th>1</th>
			<th>1</th>
			<th>1</th>
		</tr>
	    <% results.forEach(function(r) { %>
	    	<tr>
	    		<td><%= r.id %></td>
	    		<td><%= r.start_instant %></td>
	    		<td><%= r.city_main %></td>
	    		<td><button onclick="showRecord(<%=r.id %>)">Show Record </button></td>
	    		<td><button onclick="deleteRecord(<%=r.id %>)">Delete Record </button></td>
	    	</tr>
	    <% }); %>
	</table>

	<button onclick="addRecord()">Add Record</button>
	
</div>
</body>

<script type="text/javascript" src="javascript/metrics.js"></script>
</html>
